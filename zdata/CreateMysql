create table pvtable(
id int primary key auto_increment,
webid varchar(100),
cookieid varchar(500),
ip varchar(100),
phone varchar(100),
devicetype varchar(100),
url varchar(500),
title varchar(500),
referrer varchar(500),
res varchar(500),
colordepth varchar(500),
lang varchar(500),
brower varchar(500),
agent text,
country varchar(50) comment '国家',
area varchar(50) comment '地区',
region varchar(50) comment '省份',
city varchar(100) comment '城市',
county varchar(100) comment '县',
isp varchar(100) comment '运营商',
date datetime
);
ALTER TABLE pvtable ADD INDEX go ( `ip`, `date`);

create table shares(
id int primary key auto_increment comment '用户ID',
webid varchar(100),
cookieid varchar(500),
channel varchar(100),
url text,
phone varchar(100),
ip varchar(100),
devicetype varchar(100),
domain text,
referrer text,
title text,
uri text,
res varchar(200),
colordepth varchar(500),
lang varchar(100),
brower varchar(100),
agent text,
country varchar(50) comment '国家',
area varchar(50) comment '地区',
region varchar(50) comment '省份',
city varchar(100) comment '城市',
county varchar(100) comment '县',
isp varchar(100) comment '运营商',
date datetime
);